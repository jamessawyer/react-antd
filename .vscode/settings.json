{
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "prettier.disableLanguages": ["js"],
  "javascript.suggestionActions.enabled": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "dist/*": true,
    "*.log": true,
    "yarn.lock": true
  }
}
